{
    "name":"Volume Control",
    "version": "0.1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
          "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>"
    ]
}